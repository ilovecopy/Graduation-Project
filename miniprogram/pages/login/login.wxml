<van-cell-group>
  <van-field value="{{ username }}" required clearable label="用户名" icon="question-o" placeholder="请输入用户名" bind:click-icon="onClickIcon" bind:input="getAccount" />
  <van-field value="{{ password }}" type="password" label="密码" placeholder="请输入密码" required border="{{ false }}" bind:input="getPassword" />
</van-cell-group>
<button bindtap="login" class="button">
  <text class="button_text">登陆</text>
</button>